{
    "coc.source.OmniSharp.enable": true,
    "coc.source.OmniSharp.triggerCharacters": ".",
    "coc.preferences.extensionUpdateCheck": "daily",
    "coc.preferences.currentFunctionSymbolAutoUpdate": true,
    "coc.preferences.bracketEnterImprove": true,
    "coc.preferences.hoverTarget": "float",
    "coc.preferences.formatOnSaveFiletypes": [
        "yaml",
        "css",
        "js"
    ],
    "snippets.loadFromExtensions": true,
    "snippets.ultisnips.enable": true,
    "snippets.ultisnips.directories": [
        "UltiSnips",
        "~/.config/nvim/snips"
    ],
    "suggest.enablePreselect": false,
    "suggest.numberSelect": false,
    "suggest.languageSourcePriority": 99,
    "suggest.noselect": false,
    "suggest.lowPrioritySourceLimit": 5,
    "suggest.triggerAfterInsertEnter": true,
    "suggest.snippetIndicator": " â†’",
    "suggest.acceptSuggestionOnCommitCharacter": true,
    "suggest.minTriggerInputLength": 1,
    "languageserver": {
        "ccls": {
            "command": "ccls",
            "filetypes": [
                "c",
                "cpp",
                "cc",
                "h",
                "hh",
                "hpp",
                "objc",
                "objcpp"
            ],
            "rootPatterns": [
                ".ccls",
                "compile_commands.json",
                ".vim/",
                ".git/",
                ".hg/"
            ],
            "initializationOptions": {
                "cache": {
                    "directory": "/tmp/ccls"
                }
            }
        },
        "go": {
            "command": "gopls",
            "rootPatterns": [
                "go.mod"
            ],
            "trace.server": "verbose",
            "filetypes": [
                "go"
            ]
        },
        "terraform": {
            "command": "terraform-ls",
            "args": [
                "serve"
            ],
            "filetypes": [
                "terraform",
                "tf"
            ],
            "initializationOptions": {},
            "settings": {}
        }
    }
}
